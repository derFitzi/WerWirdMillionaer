<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.werwirdmillionaer.MainMenuController">
<VBox fx:id="vbox" alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" spacing="50.0">
  <Button fx:id="playButton" graphicTextGap="15.0" minHeight="50.0" minWidth="250.0" onAction="#startGame" text="Play">
         <font>
            <Font name="ArtifaktElement-Medium" size="18.0" />
         </font></Button>
   <Button fx:id="settingsButton" graphicTextGap="15.0" minHeight="50.0" minWidth="250.0" text="Settings">
         <font>
            <Font name="ArtifaktElement-Medium" size="18.0" />
         </font>
      </Button>
  <Button fx:id="exitButton" graphicTextGap="15.0" minHeight="50.0" minWidth="250.0" onAction="#exitGame" text="Exit">
         <font>
            <Font name="ArtifaktElement-Medium" size="18.0" />
         </font></Button>
</VBox>
</AnchorPane>
